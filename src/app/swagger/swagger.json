              "format": "email",
              "nullable": true
            },
            "password": {
              "maxLength": 15,
              "minLength": 8,
              "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[^\\da-zA-Z]).{8,15}$",
